<h1>Sign Up</h1>

<%# raise @user.inspect %>
<%= form_for(User.new) do |f| %>

  <%= f.label :full_name %>
  <%= f.text_field :full_name %> <br>

  <%= f.label :display_name %>
  <%= f.password_field :display_name %> <br>
  
  <%= f.label :email %>
  <%= f.text_field :email %> <br>

  <%= f.label :password %>
  <%= f.password_field :password %> <br>
  
  <%= f.submit "Sign up" %>
<% end %>
